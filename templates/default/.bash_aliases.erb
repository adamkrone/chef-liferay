alias lrhome='cd <%= @liferay_home %>'

alias lrtail='sudo -u liferay tail -f <%= @liferay_log_home %>/catalina.out'

alias lrstart='sudo /etc/init.d/liferay start'

alias lrstop='sudo /etc/init.d/liferay stop'

alias lrclear='sudo rm <%= @liferay_log_home %>/*'

alias lrps='sudo ps aux | grep java'

alias lrdeploy='sudo -u liferay cp /vagrant/dist/* <%= @liferay_home %>/deploy/.'
